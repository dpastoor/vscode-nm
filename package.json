{
    "name": "vscode-nm",
    "displayName": "nonmem",
    "description": "nonmem syntax highlighter",
    "version": "0.2.0",
    "publisher": "dpastoor",
    "bugs": {
        "url": "https://github.com/dpastoor/vscode-nm/issues"
    },
    "homepage": "https://github.com/dpastoor/vscode-nm/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/dpastoor/vscode-nm.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "nm7",
                "aliases": [
                    "nm7",
                    "nm7"
                ],
                "extensions": [
                    ".mod",
                    ".ctl",
                    ".lst"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nm7",
                "scopeName": "source.nm7",
                "path": "./syntaxes/nm7.tmLanguage"
            }
        ]
    }
}